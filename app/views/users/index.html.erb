<h1>ユーザー一覧</h1>

<table class="table table-bordered">
  <thead>
    <tr>
      <th>ユーザー名</th>
      <th>メールアドレス</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @users %>
